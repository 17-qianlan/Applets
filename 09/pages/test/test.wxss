.song-list {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.song-list .song-list-item {
  width: 32%;
  padding: 0 16rpx 10rpx 0;
}
.song-list .song-list-item image {
  width: 100%;
  height: 100%;
}
.song-list .song-list-item text {
  display: block;
  text-align: left;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.song-list .song-list-item:nth-child(3n) {
  padding: 0;
}

scroll-view {
  height: 100vh;
}

