.search {
  width: 100%;
  background: #b3d4db;
}
.search form {
  display: block;
  width: 90%;
  padding: 5rpx 20rpx;
  margin: auto;
  overflow: hidden;
}
.search form input {
  width: 89%;
  height: 30rpx;
  border-bottom: 2rpx solid #fff;
}
.search form button:after {
  border: 0;
}
.search form button {
  width: 60rpx;
  height: 60rpx;
  padding: 0;
  margin: 0 10rpx 0 0;
  background: transparent;
}
.search form button image {
  display: block;
  width: 100%;
  height: 100%;
}

