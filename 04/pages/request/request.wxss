.container {
  width: 750rpx;
  height: 100vh;
  background: #f5f5f5;
}
.container .bg {
  width: 100%;
  height: 200rpx;
}
.container .bg image {
  width: 100%;
  height: 100%;
}
.container .form {
  width: 100%;
  height: 400rpx;
}
.container .form form {
  display: block;
}
.container .form form .address {
  display: flex;
  width: 95%;
  height: 100rpx;
  padding: 0 10rpx;
}
.container .form form .address input {
  width: 285rpx;
  height: 100rpx;
  border-bottom: 1rpx solid #ccc;
  font-size: 35rpx;
}
.container .form form .address image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  border: 1rpx solid #e6e6e6;
  margin: 10rpx 20rpx 0;
  line-height: 100rpx;
}
.container .form form tiem {
  margin-top: 20rpx;
}
.container .form form .submit {
  width: 100%;
  height: 100rpx;
  margin-top: 50rpx;
  background: #ff9a14;
}

