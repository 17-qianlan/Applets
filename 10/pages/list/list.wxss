.song-list {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.song-list .song-list-item {
  width: 31%;
  padding-bottom: 10rpx;
}
.song-list .song-list-item image {
  width: 100%;
  height: 100%;
}
.song-list .song-list-item text {
  display: block;
  text-align: left;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

scroll-view {
  display: block;
  height: 100vh;
}
scroll-view .song-list {
  padding: 20rpx;
  box-sizing: border-box;
}

.player {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  padding: 10rpx;
  overflow: hidden;
  z-index: 1000;
}
.player .song-information {
  display: block;
  width: 400rpx;
  margin-left: 10rpx;
}
.player .song-information text {
  display: block;
}
.player .song-information text:nth-child(2) {
  margin: 10rpx 0 0 10rpx;
  font-size: 30rpx;
}
.player .next {
  margin-right: 20rpx;
}
.player .play {
  margin-right: 20rpx;
}
.player image {
  width: 100rpx;
  height: 100rpx;
}

