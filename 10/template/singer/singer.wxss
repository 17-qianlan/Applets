.msg {
  width: 100%;
  height: 970rpx;
  padding: 10rpx 0;
}
.msg .photo {
  position: relative;
  width: 100%;
  height: 900rpx;
}
.msg .photo image {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 450rpx;
  height: 450rpx;
  border-radius: 50%;
}
.msg .details {
  display: flex;
  width: 85%;
  height: 70rpx;
  margin: auto;
}
.msg .details view {
  flex: 1;
  color: #ccc;
  font-size: 60rpx;
  text-align: center;
  font-weight: normal;
}

